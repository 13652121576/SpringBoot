<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ydm.springboot.dao.CityDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.ydm.springboot.entity.City" id="CityMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="parentid" column="parentId"/>
        <result property="level" column="level"/>
        <result property="szm" column="szm"/>
        <result property="pingyin" column="pingyin"/>
    </resultMap>

    <select id="findAll" resultMap="CityMap">
        SELECT name FROM mjb_city
    </select>


</mapper>